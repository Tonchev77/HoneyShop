@model IEnumerable<HoneyShop.ViewModels.Home.GetAllCategoriesViewModel>

<ul class="depart-hover">
    @foreach (var category in Model)
    {
        <li>
            <a asp-controller="Shop" asp-action="Index" asp-route-categoryId="@category.Id">
                @category.Name
            </a>
        </li>
    }
</ul>